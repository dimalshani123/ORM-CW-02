<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lk.ijse.controller.ViewRegistrationsFormController">

    <children>
        <Text fx:id="studentHeading" text="View Registered Students" layoutX="20.0" layoutY="30.0" style="-fx-font-size: 20; -fx-font-weight: bold;" />

        <TableView fx:id="viewTbl" layoutX="20.0" layoutY="50.0" prefWidth="560.0" prefHeight="300.0">
            <columns>
                <TableColumn fx:id="colStudentId" text="Student ID" prefWidth="180.0" />
                <TableColumn fx:id="colCoordinatorId" text="Coordinator ID" prefWidth="180.0" />
                <TableColumn fx:id="colStudentName" text="Student Name" prefWidth="200.0" />
            </columns>
        </TableView>

        <Button fx:id="viewBtn" text="View Students" layoutX="20.0" layoutY="370.0" prefWidth="150.0" onAction="#viewBtnOnAction" />

    </children>
</AnchorPane>
